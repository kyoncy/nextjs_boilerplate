{
  "name": "next_boilerplate",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:es",
    "lint:es": "eslint '**/*.{ts,tsx}'",
    "format": "prettier --write . && npm run lint:es -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ress": "4.0.0",
    "sass": "^1.49.7"
  },
  "devDependencies": {
    "@types/node": "17.0.13",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "eslint": "8.8.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  }
}
